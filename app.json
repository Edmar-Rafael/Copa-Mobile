{
  "expo": {
    "name": "copa-mobile",
    "scheme": "my-mobile-app",
    "slug": "copa-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#333"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
